@echo off
echo "DOCS PUSH BAT"

echo "1. move to D:\typora_work\Latex"
D:
cd D:\typora_work\Latex
set now=%date% %time%
echo "Time:" %now% >> abc.txt


echo "2. git add命令可将该文件添加到暂存区."
git add *

echo "3. git commit 命令用来将本地暂存的修改提交到版本库."

git commit -m "%now%"

echo "4. git push 命用于从将本地的分支版本上传到远程并合并."
git push origin master:main

echo "打卡成功"
